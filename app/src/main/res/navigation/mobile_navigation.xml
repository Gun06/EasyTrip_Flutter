<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/mobile_navigation"
  app:startDestination="@+id/navigation_home">

  <fragment
    android:id="@+id/navigation_review"
    android:name="com.example.easytrip_test.ui.review.ReviewFragment"
    android:label="@string/title_review"
    tools:layout="@layout/fragment_review" />

  <fragment
    android:id="@+id/navigation_schedule"
    android:name="com.example.easytrip_test.ui.schedule.ScheduleFragment"
    android:label="@string/title_schedule"
    tools:layout="@layout/fragment_schedule" />

  <fragment
    android:id="@+id/navigation_home"
    android:name="com.example.easytrip_test.ui.home.HomeFragment"
    android:label="@string/title_home"
    tools:layout="@layout/fragment_home" />

  <fragment
    android:id="@+id/navigation_traffic"
    android:name="com.example.easytrip_test.ui.traffic.TrafficFragment"
    android:label="@string/title_traffic"
    tools:layout="@layout/fragment_traffic" />

  <fragment
    android:id="@+id/navigation_mypage"
    android:name="com.example.easytrip_test.ui.mypage.MypageFragment"
    android:label="@string/title_mypage"
    tools:layout="@layout/fragment_mypage" />


</navigation>